        "package": "model",
        "processing-order": [
            "settings",
            "*",
            "model"
        ],
        "package-base": "model",
        "package-base-upper": "MODEL",
        "kebap-package": "model",
        "package-path": "model",
        "schema-version": "1.0.0",
        "schema-name": "model-1.0.0.json",
        "schema-file": "model/records/jsonschemas/model-1.0.0.json",
        "mapping-package": "model.records.mappings",
        "jsonschemas-package": "model.records.jsonschemas",
        "mapping-file": "model/records/mappings/v7/model/model-1.0.0.json",
        "schema-server": "http://localhost/schemas/",
        "index-name": "model-model-1.0.0",
        "collection-url": "/model/",
        "model-name": "model",
        "saved-model-file": "model/models/model.json",
        "inherited-model-file": "model/models/inherited_model.json",
        "script-import-sample-data": "scripts/sample_data.yaml",
        "elasticsearch": {
            "version": "v7",
            "templates": {
                "v7": {
                    "mappings": {
