{
    "version": "1.0.0",
    "model": {
        "properties": {
            "metadata": {
                "properties": {
                    "updated": {
                        "type": "date",
                        "required": true,
                        "oarepo:sample": {
                            "faker": "iso8601",
                            "skip": false
                        }
                    }
                },
                "oarepo:marshmallow": {
                    "class": "model.services.schema.ModelMetadataSchema",
                    "base-classes": [
                        "ma.Schema"
                    ],
                    "generate": true
                },
                "type": "object"
            },
            "id": {
                "type": "keyword",
                "oarepo:sample": {
                    "skip": true
                },
                "oarepo:marshmallow": {
                    "read": false,
                    "write": false
                }
            },
            "created": {
                "type": "date",
                "oarepo:sample": {
                    "skip": true
                },
                "oarepo:marshmallow": {
                    "write": false,
                    "read": true
                }
            },
            "updated": {
                "type": "date",
                "oarepo:sample": {
                    "skip": true
                },
                "oarepo:marshmallow": {
                    "write": false,
                    "read": true
                }
            },
            "$schema": {
                "type": "keyword",
                "oarepo:marshmallow": {
                    "read": false,
                    "write": false
                },
                "oarepo:sample": {
                    "skip": true
                }
            }
        },
        "oarepo:marshmallow": {
            "base-classes": [
                "invenio_records_resources.services.records.schema.BaseRecordSchema"
            ],
            "class": "model.services.schema.ModelSchema",
            "generate": true
        },
        "type": "object"
    },
    "settings": {
        "python": {
            "use_isort": true,
            "use_black": true,
            "record-prefix": "Model",
            "templates": {},
            "marshmallow": {
                "mapping": {}
            },
            "record-prefix-snake": "model",
            "record-resources-package": "model.resources",
            "record-services-package": "model.services",
            "record-records-package": "model.records",
            "config-package": "model.config",
            "config-dummy-class": "model.config.DummyClass",
            "config-resource-config-key": "MODEL_RESOURCE_CONFIG",
            "config-resource-class-key": "MODEL_RESOURCE_CLASS",
            "config-service-config-key": "MODEL_SERVICE_CONFIG",
            "config-service-class-key": "MODEL_SERVICE_CLASS",
            "config-resource-register-blueprint-key": "MODEL_REGISTER_BLUEPRINT",
            "ext-class": "model.ext.ModelExt",
            "flask-extension-name": "model",
