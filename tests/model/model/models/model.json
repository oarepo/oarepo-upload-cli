            "record-mapping-setup-cfg": "model",
            "record-jsonschemas-setup-cfg": "model",
            "record-resource-config-class": "model.resources.config.ModelResourceConfig",
            "record-resource-class": "model.resources.resource.ModelResource",
            "record-permissions-class": "model.services.permissions.ModelPermissionPolicy",
            "record-service-class": "model.services.service.ModelService",
            "record-service-config-class": "model.services.config.ModelServiceConfig",
            "record-service-config-bases": [],
            "record-service-config-generate-links": true,
            "record-schema-class": "model.services.schema.ModelSchema",
            "record-schema-metadata-class": "model.services.schema.ModelMetadataSchema",
            "record-dumper-class": "model.records.dumper.ModelDumper",
            "record-search-options-class": "model.services.search.ModelSearchOptions",
            "record-facets-class": "model.services.facets.Test",
            "record-schema-metadata-alembic": "model",
